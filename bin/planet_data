#!/usr/bin/env ruby

$LOAD_PATH << File.expand_path("../../lib", __FILE__)

require 'planet'

Birach  = Planet.new('354623')
Earth   = Planet.new('867557')

#puts "Birach's gravity is #{'%.3f' % Birach.gravity}."
#puts "Earth's gravity is #{'%.3f' % Earth.gravity}."

puts Earth.radius
puts Earth.atmo_mod()
puts Birach.density
puts Earth.density
